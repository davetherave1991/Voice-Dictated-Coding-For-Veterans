Py create mul  = '>>> X * Y';
Py create div = '>>> X / Y';

#Python Con
Py STR OP ='>>> print ("String-1"+","+"String-2")';

#Python Variables
Py Var N = '>>> x = 100';
Py Var S = '>>> x = "string"';
Py Var named = '>>> named_variable_x = var';

#Python input
Py Var input = '>>> x = input("Enter a number:"){enter}>>>print(x)';

#Python Booleans
Py B T = '>>> my_boolean = True';
Py B false comp = '>>> X == not-x';

#Python Comparisons
Py not EQ = 'X != Y';
Py GR = 'X > Y';
Py LT ='X < Y';
Py EQ GR = 'X >= Y';
Py EQ LT = 'X <= Y';

